<%= render 'template_ones/edit_partials/portfolio_carousel_heading' %> 


<div class='container'> 
<%= bootstrap_flash %>  
<%= simple_form_for(@template_one) do |f| %>


  <div class="form-inputs">
    <%= f.input :portfolio_header_hide, label: "Hide this module on main page?" %> 
    <%= f.input :portfolio_header_title %>
    <%= f.input :portfolio_header_text %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, data: {disable_with: 'One moment please...'} %> 
     | <%= link_to "Home", root_path, class: "btn btn-default" %> 
  </div>
<% end %>
</div>